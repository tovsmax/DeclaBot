{
  "name": "declabot",
  "version": "0.1.0",
  "type": "module",
  "dependencies": {
    "axios": "^0.27.2",
    "dotenv": "^16.0.1",
    "express": "^4.18.1",
    "localtunnel": "^2.0.2",
    "multer": "^1.4.5-lts.1",
    "node-telegram-bot-api": "^0.57.0",
    "swagger-autogen": "^2.21.1",
    "swagger-ui-express": "^4.3.0",
    "telegraf": "^4.8.1"
  },
  "devDependencies": {
    "nodemon": "^2.0.16"
  },
  "scripts": {
    "check": "netstat -ano | findstr :3000",
    "gendoc": "node -r dotenv/config swagger/gendoc --skip",
    "dev-gendoc": "nodemon -r dotenv/config --ignore swagger/output.json swagger/gendoc",
    "dev": "nodemon -r dotenv/config server/server",
    "start-gendoc": "node -r dotenv/config swagger/gendoc",
    "start": "node -r dotenv/config server/server"
  }
}
